			sInfo[playerid][Misc] = 0;
			sInfo[playerid][Head] = 0;
			sInfo[playerid][Chest] = 0;
			sInfo[playerid][Crotch] = 0;
			sInfo[playerid][LArm] = 0;
			sInfo[playerid][RArm] = 0;
			sInfo[playerid][LLeg] = 0;
			sInfo[playerid][RLeg] = 0;
			killed[playerid] = 0;
         }
