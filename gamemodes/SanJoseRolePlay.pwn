				}
				case 4:
				{
					if(strlen(params) < 1 || strlen(params) > 8)
					{
						ShowErrorDialog(playerid, "Invalid length!");
						return 1;
					}
					strmid(VehicleNumberPlate[id], params, 0, sizeof(params));
