    INI_WriteFloat(File, "hExitY", HouseInfo[id][hExitY]);
    INI_WriteFloat(File, "hExitZ", HouseInfo[id][hExitZ]);
    INI_WriteFloat(File, "hExitA", HouseInfo[id][hExitA]);
