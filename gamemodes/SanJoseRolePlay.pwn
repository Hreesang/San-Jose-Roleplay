    new string[144];

    if(strlen(params) > 64)
    {
        new text1[65], 
            text2[65];

        strmid(text2, params, 64, 128);
        strmid(text1, params, 0, 64);

        format(string, 128, "(( [OOC:] %s: %s ...", GetName(playerid), text1);
        SendClientMessageToAll(COLOR_OOC, string);

        format(string, 128, "... %s ))", text2);
